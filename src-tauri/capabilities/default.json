{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-ask",
    "notification:default",
    "os:default",
    "process:default",
    "process:allow-relaunch",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install"
  ]
}